            <check_task type="task_table"  task_query="select task_type,task_queue_event.start_time,task_id from task_queue_event join vm_data on vm_data.id=task_queue_event.vm_id where vm_data.vm_name=%s and task_queue_event.task_type=%s order by task_queue_event.start_time desc">Create VM</check_task>
