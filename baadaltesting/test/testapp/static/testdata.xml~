		<test_case1 value="Checking for correct entries in My VMs Table as User(UA) " query3="select vm_data.vm_name,concat(user1.first_name,' ',user1.last_name) AS user_name,host.host_ip,vm_data.private_ip,vm_data.public_ip,vm_data.RAM,vm_data.vCPU,vm_data.status from user as user1,user_vm_map,vm_data,user,host where user1.id=vm_data.owner_id and  vm_data.host_id=host.id and user.id=user_id and user_vm_map.vm_id=vm_data.id and user.username='badalUA' and (vm_data.status=2 OR vm_data.status=3 OR vm_data.status=4)">
			<user_name id="user_username" type="input">badalUA</user_name>
			<password id="user_password" type="input">baadal</password>
			
